import React, { useState } from 'react';
import { Card, CardContent, Typography, Box, IconButton, Tooltip, Button } from '@mui/material';
import InstagramIcon from '@mui/icons-material/Instagram';
import LinkIcon from '@mui/icons-material/Link';
import FavoriteIcon from '@mui/icons-material/Favorite';
import { styled } from '@mui/system';

const brandData = {
  logo: 'https://bluorng.com/cdn/shop/files/DSC05165173.jpg?v=1693989827&width=823', 
  website: 'https://example.com',
  instagramId: '@homegrown_brand',
  brandName: 'Homegrown Brand',
  description: 'This is a fantastic brand that offers stylish and comfortable streetwear...',
  products: [
    'https://i.pinimg.com/originals/59/04/45/590445d384a67066ff5bdbcf00d7bea6.jpg', 
    'https://i.pinimg.com/originals/15/26/2f/15262fc580161600c21f2b60ae7234b5.jpg',
    'https://i.pinimg.com/originals/4f/a9/bf/4fa9bfe836a2b0e08b5ad4db2f7dc9ce.jpg',
    'https://i.pinimg.com/originals/b8/53/a9/b853a996f80842763f30295379f5c575.jpg'
  ],
  reviews: 850,
  likes: 980
};

const CarouselContainer = styled('div')({
  display: 'flex',
  justifyContent: 'center',
  alignItems: 'center',
  position: 'relative',
  width: '100%',
  height: '200px',
  perspective: '1000px',
  overflow: 'hidden',
});

const ImageWrapper = styled(Box)(({ index, currentIndex }) => ({
  width: '100px', 
  height: '150px',
  transform: `scale(${index === currentIndex ? 1.2 : 0.8}) rotateY(${index < currentIndex ? '30deg' : '-30deg'})`,
  transition: 'transform 0.5s',
  opacity: index === currentIndex ? 1 : 0.5, // Side images are transparent
  cursor: 'pointer',
  position: index === currentIndex ? 'relative' : 'absolute',
  left: index === currentIndex ? '50%' : index < currentIndex ? '10%' : '90%',
}));

const ArrowButton = styled(IconButton)({
  position: 'absolute',
  top: '50%',
  transform: 'translateY(-50%)',
  backgroundColor: 'rgba(0, 0, 0, 0.3)',
  color: '#fff',
});

const BrandCard = ({ 
  logo = brandData.logo, 
  instagramId = brandData.instagramId, 
  brandName = brandData.brandName, 
  description = brandData.description, 
  products = brandData.products, 
  website = brandData.website, 
  reviews = brandData.reviews, 
  likes = brandData.likes 
}) => {
  
  const [currentIndex, setCurrentIndex] = useState(0);
  const handleNext = () => setCurrentIndex((prev) => (prev + 1) % products.length);
  const handlePrev = () => setCurrentIndex((prev) => (prev - 1 + products.length) % products.length);

  return (
    <Card sx={{ width: 420, height: '70vh', position: 'relative', m: 2 }}>
      {/* Logo */}
      <Box component="img" src={logo} alt={brandName} sx={{ width: '100%', height: '150px', objectFit: 'cover' }} />

      {/* Website Link with Tooltip */}
      <Tooltip title={`Visit ${brandName} website`}>
        <IconButton component="a" href={website} target="_blank" rel="noopener noreferrer"
          sx={{ position: 'absolute', top: 8, right: 8, backgroundColor: 'rgba(255,255,255,0.9)', borderRadius: '50%', p: 1 }}>
          <LinkIcon />
        </IconButton>
      </Tooltip>

      <CardContent>
        {/* Instagram */}
        <Box sx={{ display: 'flex', alignItems: 'center', mb: 1 }}>
          <InstagramIcon fontSize="small" sx={{ mr: 1 }} />
          <Typography variant="body2">{instagramId}</Typography>
        </Box>

        {/* Brand Name with Like Button */}
        <Box sx={{ display: 'flex', alignItems: 'center', mb: 1 }}>
          <Typography variant="h6" component="div" sx={{ flexGrow: 1 }}>{brandName}</Typography>
          <IconButton aria-label="like" sx={{ color: 'red' }}>
            <FavoriteIcon />
          </IconButton>
        </Box>

        {/* Description */}
        <Typography variant="body2" color="text.secondary" sx={{ mb: 2 }}>{description}</Typography>

        {/* Carousel */}
        <CarouselContainer>
          <ArrowButton onClick={handlePrev} sx={{ left: 10 }}>&lt;</ArrowButton>
          {products.map((product, index) => (
            <ImageWrapper key={index} index={index} currentIndex={currentIndex}>
              <Box component="img" src={product} alt={`product-${index}`} sx={{ width: '100%', height: '100%', objectFit: 'cover' }} />
            </ImageWrapper>
          ))}
          <ArrowButton onClick={handleNext} sx={{ right: 10 }}>&gt;</ArrowButton>
        </CarouselContainer>

        {/* Review Button with Tooltip */}
        <Tooltip title="Leave a Review" placement="right">
          <Button 
            variant="contained" 
            color="primary" 
            sx={{ mt: 2, textTransform: 'none', borderRadius: '0', width: '100%' }} 
            onClick={() => console.log('Redirect to brand review page')}
          >
            Review This Brand
          </Button>
        </Tooltip>

        {/* Reviews & Likes in separate lines */}
        <Box sx={{ display: 'flex', justifyContent: 'space-between', mt: 2 }}>
          <Typography variant="body2">{reviews} reviews</Typography>
          <Typography variant="body2">{likes} likes</Typography>
        </Box>
      </CardContent>
    </Card>
  );
};

export default BrandCard;
